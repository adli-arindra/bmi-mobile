{
  "expo": {
    "name": "bmi-mobile",
    "slug": "bmi-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dino.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adliarindra.bmimobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#BBF7D2"
      },
      "package": "com.adliarindra.bmimobile"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/dino.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#BBF7D2"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4f580969-7d23-4660-bdfe-2532240d814c"
      }
    }
  }
}
